            "GPSOptysbyTeamMember_1": {
                "broadcastFacet": true,
                "groups": [],
                "label": "GPSOptysbyTeamMember",
                "numbers": [],
                "query": "q = load \"Load_opty_services_acct_all\";\nq = filter q by 'OTM.UserId' in {{column(gCRM_User__c_1.selection, [\"gCRM_User__c\"]).asString()}};\nq = filter q by 'OpportunityServices__c.gCRM_GPS_Stage_US__c' in {{column(Opty_gCRM_GPS_Stage__1.result, [\"Opty.gCRM_GPS_Stage_US__c\"]).asString()}};\nq = group q by ('OpportunityServices__c.OwnerId', 'OTM.UserId', 'OpportunityServices__c.gCRM_Opportunity_ID__c', 'OpportunityServices__c.gCRM_Reason_for_Won_Lost_Abandoned__c', 'OpportunityServices__c.gCRM_GPS_Stage_US__c');\nq = foreach q generate q.'OpportunityServices__c.OwnerId' as 'Leader', q.'OTM.UserId' as 'Team Member', q.'OpportunityServices__c.gCRM_Opportunity_ID__c' as 'OpportunityServices__c.gCRM_Opportunity_ID__c', 'OpportunityServices__c.gCRM_Reason_for_Won_Lost_Abandoned__c' as 'Primary Closed Reason', q.'OpportunityServices__c.gCRM_GPS_Stage_US__c' as 'GPS Stage (US)', sum(q.'OpportunityServices__c.gCRM_Amount_USD__c') as 'SUM of Sales Amount', sum(q.'OpportunityServices__c.SalesCredit.gCRM_Sales_Credit_Amount__c') as 'SUM of Sales Credit';\nq = order q by ('Leader' asc, 'Team Member' asc,'OpportunityServices__c.gCRM_Opportunity_ID__c' asc ,'GPS Stage (US)' desc);\nq = limit q 2000;",
